<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Music Player - CodePen</title>

<link rel="stylesheet" href="css/reset.css">

<link rel="stylesheet" href="css/style.css" media="screen" type="text/css" />



</head>

<body>
	
	
	

<div id="hi">Henry's music player</div>
<script>
function ct(){
//var colors = new Array("#FFD700","#00FF00","#00F5FF","#FFEFD5","#6495ED","#FA8072","#FFA54F");
var obj = document.getElementById("hi");
if (obj.style.color == "#FFD700") {
	obj.style.color = "#00FF00";
		setTimeout("ct()",1000);
	}
else if (obj.style.color == "#00FF00") {
	obj.style.color = "#00F5FF";
		setTimeout("ct()",1000);
		}
else if (obj.style.color == "#00F5FF") {
	obj.style.color = "#FFEFD5";
		setTimeout("ct()",1000);
	}
else if (obj.style.color == "#FFEFD5") {
	obj.style.color = "#6495ED";
		setTimeout("ct()",1000);
	}
else if (obj.style.color == "#6495ED") {
	obj.style.color = "#FA8072";
		setTimeout("ct()",1000);
	}
else if (obj.style.color == "#FA8072") {
	obj.style.color = "#FFA54F";
		setTimeout("ct()",1000);
	}
else (obj.style.color == "#FFA54F") {
	obj.style.color = "#FFD700";
		setTimeout("ct()",1000);
	}

//  obj.style.color = obj.style.color==""?("#FFEFD5"?("#6495ED"?"#00FF00":""):""):"";
	setTimeout("ct()",1000);
}
ct();
</script>
<div class="ac">
  <div class="music-player">
  <!--<div style="background-image: url(http://i.imgur.com/yqB0erk.jpg);" class="album"></div>-->

  
    <div class="info">


      <div class="jp-playlist">
        <ul>
          <li></li>
        </ul>
      </div>
      


      <div class="progress jp-seek-bar">
        <span class="jp-play-bar" style="width: 0%"></span>
      </div>
    </div>
   <div class="controls">
    <div class="current jp-current-time">00:00</div>
    <div class="play-controls">
      <a href="javascript:;" class="icon-previous jp-previous" title="previous"></a>
      <a href="javascript:;" class="icon-play jp-play" title="play"></a>
      <a href="javascript:;" class="icon-pause jp-pause" title="pause"></a>
      <a href="javascript:;" class="icon-next jp-next" title="next"></a>
    </div>
    <div class="volume-level jp-volume-bar">
      <span class="jp-volume-bar-value" style="width: 0%"></span>
      <a href="javascript:;" class="icon-volume-up jp-volume-max" title="max volume"></a>
      <a href="javascript:;" class="icon-volume-down jp-mute" title="mute"></a>
    </div>
    <div id="jquery_jplayer" class="jp-jplayer"></div>
   </div>
  </div>
  
  
  

  
  <div class="music-player0">
	  <div class="info">	
        <div class="jp-playlist">
          <ul>
            <li></li>
          </ul>
        </div>

    
    </div>
  </div>


<script src="js/jquery.min.js"></script>
<script src="js/jquery.jplayer.min.js"></script>
