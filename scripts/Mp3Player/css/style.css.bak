html {
  min-height: 100%;
}

body {
  background: #eee url("../images/82fLDu4.jpg") no-repeat center;
  background-size: cover;
  font-family: "Open Sans", sans-serif;
}


#hi {
	position: relative;
	font: 1.5em "¿¬Ìå";
	color:  gold;
	text-align:  center;
	margin: 100px auto auto auto;
}



.ac {
  position: relative;
  width: 710px;
  height: 290px;
  margin: 100px auto auto 250px;
  box-shadow: 0 0 60px rgba(192,192,192, 0.8);
  border-radius: 10px;
  background: rgba(135 206 235,0.5);/*#C0C0C0*/
  overflow: hidden;
  z-index: 0;
}


.music-player {
  position: relative;
  width: 350px;
  height: 290px;
  margin: 0px auto auto 5px;
  box-shadow: 0 0 60px rgba(192,192,192, 0.8);
  border-radius: 10px;
  background: rgba(135 206 235,0.5);
  overflow: hidden;
  z-index: 0;
}
.music-player img {
  position: absolute;
  top: 0px;
  left: 0px;
  bottom: 0px;
  right: 0px;
  z-index: -1;
  display: block;
  width: 100% !important;
  height: 100% !important;
  -webkit-filter: blur(2px);
          filter: blur(2px);
}

.music-player0 {
  position: absolute;
  width: 350px;
  height: 290px;
  margin: auto auto auto auto;
  box-shadow: 0 0 60px rgba(192,192,192, 0.8);
  border-radius: 10px;
  background: #C0C0C0;
  overflow: hidden;
  z-index: 0;
}



.music-player .info {
	text-align: center;
  width: 100%;
  height: 170px;
  background: #C0C0C0;
  background: rgba(192,192,192, 0.8);
  position: relative;
  padding: 80px 0 0;
}















.music-player .info .jp-playlist li {
  display: none;
}
.music-player .info .jp-playlist li a {
  font-size: 24px;
  font-weight: 300;
  text-decoration: none;
  color: #FF4500;
  color: rgba(255 69 0, 0.8);
}
.music-player .info .jp-playlist li a span {
  font-size: 14px;
  display: block;
  margin-top: 10px;
}
.music-player .info .jp-playlist li.jp-playlist-current {
  display: block;
}
.music-player .info .jp-playlist li .jp-free-media, .music-player .info .jp-playlist li .jp-playlist-item-remove {
  display: none;
}


.music-player .info [class^="icon-"] {
  margin: 0 0 10px;
}

.music-player .progress, .music-player .volume-level {
  width: 100%;
  height: 5px;
  display: None;
  background: #FFFFFF;
  position: absolute;
  bottom: 0px;
  cursor: pointer;
}
.music-player .progress span, .music-player .volume-level span {
  position: relative;
  display: block;
  background: #ed553b;
  width: 0%;
  height: 5px;
}
.music-player .progress span:after, .music-player .volume-level span:after {
  position: absolute;
  right: -5px;
  top: -8px;
  content: "";
  width: 10px;
  height: 22px;
  background: url("../images/tsqwz1N.png") no-repeat center;
}
.music-player .controls {
  text-align: center;
  width: 100%;
  height: 120px;
  background: #982e4b;
  background: rgba(152, 46, 75, 0.6);
}
.music-player .controls .current {
  font-size: 20px;
  color: #fff;
  color: rgba(225, 225, 225, 0.4);
  padding: 15px 0 20px;
}
.music-player .controls .play-controls a {
  display: inline-block;
  width: 24px;
  height: 12px;
  margin: 0 30px;
}
.music-player .controls .volume-level {
  position: relative;
  bottom: auto;
  width: 200px;
  height: 2px;
  margin: 30px auto 0;
  background: rgba(225, 225, 225, 0.3);
}
.music-player .controls .volume-level span {
  height: 2px;
}
.music-player .controls .volume-level span:after {
  right: -11px;
  top: -8px;
  width: 22px;
  height: 15px;
  background-image: url("../images/V5i67V2.png");
}
.music-player .controls .volume-level a {
  position: absolute;
  right: -32px;
  top: -8px;
  width: 22px;
}
.music-player .controls .volume-level a.icon-volume-down {
  right: auto;
  left: -25px;
}

